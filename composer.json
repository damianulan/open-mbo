{
    "name": "damianulan/open-mbo",
    "type": "project",
    "description": "Open source Management By Objectives highly scalable application. Users are able to create, categorize and assign objectives to other users or teams and evaluate their results, run MBO campaigns and assign bonus schemes and final rewards for their achievements. Open MBO also comes with robust reports and immensely adjustable forms for users to be filled in after a campaign or completely independently of it.",
    "keywords": [
        "openmbo",
        "mbo",
        "okr",
        "laravel",
        "management",
        "objectives"
    ],
    "homepage": "https://openmbo.damianulan.me",
    "license": "CC-BY-4.0",
    "authors": [
        {
            "name": "Damian UÅ‚an",
            "email": "damian.ulan@protonmail.com",
            "homepage": "https://damianulan.me",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/laravel-form-forge.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/php-enumerable.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/laravel-feature-kit.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/laravel-lucent.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/laravel-sentinel.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/laraverse.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:damianulan/php-dtos.git"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "akaunting/laravel-apexcharts": "^3.0",
        "barryvdh/laravel-snappy": "^1.0",
        "damianulan/laravel-feature-kit": "dev-main",
        "damianulan/php-enumerable": "dev-main",
        "damianulan/laravel-form-forge": "dev-main",
        "damianulan/laravel-lucent": "dev-main",
        "damianulan/laravel-sentinel": "dev-main",
        "damianulan/laraverse": "dev-main",
        "damianulan/php-dtos": "dev-main",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "fakerphp/faker": "*",
        "guzzlehttp/guzzle": "^7.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.19",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.1",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1",
        "maennchen/zipstream-php": "^2.1",
        "mews/purifier": "^3.4",
        "phpoffice/phpspreadsheet": "^1.27",
        "psr/http-message": "^1.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-backup": "^9.1",
        "spatie/laravel-markdown": "*",
        "spatie/laravel-query-builder": "^5.2",
        "spatie/laravel-settings": "^3.1",
        "spatie/laravel-tags": "^4.10",
        "spatie/laravel-translatable": "^6.11",
        "spatie/laravel-translation-loader": "^2.8",
        "spatie/shiki-php": "^2.3",
        "tjvb/laravel-mail-catchall": "^4.4",
        "yajra/laravel-datatables": "*",
        "yajra/laravel-datatables-export": "*",
        "ymigval/laravel-model-cache": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.1",
        "laravel/pint": "^1.0",
        "larastan/larastan": "^3.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N",
            "@php artisan ide-helper:meta",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\""
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dependencies": true,
    "require-dev-dependencies": true
}
